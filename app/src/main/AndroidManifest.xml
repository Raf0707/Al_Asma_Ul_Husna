<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="ru.tabiin.alasmaulhusna"
    android:versionName="7.0"
    android:versionCode="7" >
    <uses-sdk android:minSdkVersion="22" android:targetSdkVersion="33" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:name=".App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/icon_names"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AlAsmaUlHusna"
        tools:targetApi="31">
        <activity
            android:name="ru.tabiin.alasmaulhusna.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.AppTheme.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>